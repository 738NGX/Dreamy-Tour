<header tabList="{{tabList}}" bind:childPageChange="onChildPageChange" />
<block wx:if="{{selectingTour}}">
  <block wx:if="{{childPage==0}}">
    <report-statistics id = "report-statistics"/>
  
  </block>
  <block wx:elif="{{childPage==1}}">
    <report-map id = "report-map"/>

  </block>
</block>
<block wx:else>
  <scroll-view class="scrollarea" scroll-y type="list">
    <view class="container" style="height:100%; align-items: center; justify-content: center;">
      <view class="column" style="align-items: center; justify-content: center;">
        <image mode="aspectFit" style="margin:20rpx 0;" src="../../resources/yoshiko1.png" />
        <view>当前没有选择需要查看结果的行程</view>
      </view>
    </view>
  </scroll-view>
</block>